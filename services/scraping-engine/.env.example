# Configurazione Supabase per il sistema di scraping
# Copia questo file come .env e inserisci i tuoi valori reali

# Supabase Configuration
SUPABASE_URL=your-supabase-project-url
SUPABASE_ANON_KEY=your-supabase-anon-key

# Scraping Configuration (opzionali)
SCRAPING_MAX_ZONES_PER_RUN=10
SCRAPING_MIN_INTERVAL_HOURS=24
SCRAPING_MAX_LEADS_PER_ZONE=15
SCRAPING_BATCH_SIZE=3

# Per debug
DEBUG=false
DEBUG_SELECTORS=false
